// User function Template for Java
class Solution {
    public boolean isPowerOfAnother(int x, int y) {
        // code here
        boolean b=true;
        if(x==1) return y==1;
        if(y==1) return true;
        int p=1;
        while(p<y){
            p=p*x;
            if(y==p){
                return true;
            }
        }
        return false;
    }
}
