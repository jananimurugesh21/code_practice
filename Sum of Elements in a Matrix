// User function Template for Java

class Solution {
    int sumOfMatrix(int N, int M, int[][] Grid) {
        // code here
        int s=0;
        for(int i=0;i<N;i++){
            for(int j=0;j<M;j++){
                s+=Grid[i][j];
                
            }
        }
        return s;
    }
}
