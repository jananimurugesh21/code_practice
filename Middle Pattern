class GfG {
    /*method prints the given pattern in a single line */
    void printPattern(String s) {
        // Your code here
        int n=s.length();
        int mid=n/2;
        String str=s.substring(mid)+s.substring(0,mid);
        for(int i=0;i<n;i++){
            System.out.print(str.substring(0,i+1)+"$ ");
        }
    }
}
